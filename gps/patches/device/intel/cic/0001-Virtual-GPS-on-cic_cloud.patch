---
 cic_cloud/cic_cloud.mk | 7 ++++++-
 1 file changed, 6 insertions(+), 1 deletion(-)

diff --git a/cic_cloud/cic_cloud.mk b/cic_cloud/cic_cloud.mk
index 39f0068..bb68c9c 100755
--- a/cic_cloud/cic_cloud.mk
+++ b/cic_cloud/cic_cloud.mk
@@ -21,6 +21,7 @@ TARGET_AIC_MEMORY_OPT := true;
 
 PRODUCT_COPY_FILES += \
     frameworks/native/data/etc/android.hardware.opengles.aep.xml:system/vendor/etc/permissions/android.hardware.opengles.aep.xml \
+    frameworks/native/data/etc/android.hardware.location.gps.xml:system/vendor/etc/permissions/android.hardware.location.gps.xml \
     device/intel/cic/common/media_codecs.xml:system/vendor/etc/media_codecs.xml \
     device/intel/cic/cic_cloud/init.cic_cloud.rc:root/init.cic_cloud.rc
 
@@ -37,7 +38,8 @@ PRODUCT_PACKAGES += \
     libGLESv1_CM_swiftshader \
     libGLESv2_swiftshader \
     audio.r_submix.default \
-    Browser2
+    Browser2 \
+    gps.$(TARGET_PRODUCT)
 
 ifeq ($(TARGET_USE_GRALLOC_VHAL), true)
 PRODUCT_COPY_FILES += \
@@ -67,6 +69,9 @@ PRODUCT_PACKAGES += health \
     health.$(TARGET_BOARD_PLATFORM) \
     android.hardware.health@2.0-service.$(TARGET_PRODUCT)
 
+PRODUCT_PACKAGES += android.hardware.gnss@1.0-service \
+    android.hardware.gnss@1.0-impl
+
 PRODUCT_PROPERTY_OVERRIDES += \
     ro.kernel.qemu=0 \
     debug.hwui.disable=1 \
